{
  "name": "react-laravel-boilerplate",
  "version": "1.0.0",
  "scripts": {
    "dev": "npm run css-dev && npm run development",
    "css-dev": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --env.mixfile=webpack.css.js --config=node_modules/laravel-mix/setup/webpack.config.js",
    "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch": "npm run development -- --watch",
    "watch-poll": "npm run watch -- --watch-poll",
    "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --disable-host-check --config=node_modules/laravel-mix/setup/webpack.config.js",
    "prod": "npm run css-prod && npm run production",
    "css-production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --env.mixfile=webpack.css.js --config=node_modules/laravel-mix/setup/webpack.config.js",
    "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mastekator/react-laravel-boilerplate.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.10.4",
    "@types/lodash": "^4.14.165",
    "@types/react-dom": "^17.0.0",
    "@types/react-input-mask": "^3.0.0",
    "@types/react-responsive": "^8.0.2",
    "@types/yup": "^0.29.10",
    "@types/loadable__component": "^5.13.1",
    "@types/p-min-delay": "^3.0.0",
    "@types/react": "^16.9.46",
    "@types/react-helmet": "5.0.15",
    "@types/react-redux": "7.1.7",
    "@types/react-router-dom": "^5.1.5",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "axios": "^0.19.2",
    "babel-eslint": "^10.1.0",
    "babel-preset-react": "^6.24.1",
    "browser-sync-webpack-plugin": "^2.2.2",
    "cross-env": "^7.0",
    "eslint": "^7.14.0",
    "eslint-config-google": "^0.14.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.2.0",
    "laravel-mix": "^5.0.5",
    "lodash": "^4.17.19",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "7.2.0",
    "resolve-url-loader": "^3.1.2",
    "sass": "^1.15.2",
    "sass-loader": "^8.0.0",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "@hookform/resolvers": "^1.1.1",
    "@loadable/component": "^5.13.2",
    "bootstrap": "^4.5.3",
    "browser-sync": "^2.26.12",
    "connected-react-router": "^6.8.0",
    "eslint-friendly-formatter": "^4.0.1",
    "laravel-mix-react-css-modules": "^2.0.0",
    "laravel-mix-react-typescript-extension": "^1.0.3",
    "p-min-delay": "^3.1.0",
    "query-string": "^6.13.7",
    "react-bootstrap": "^1.4.0",
    "react-content-loader": "^5.1.2",
    "react-error-boundary": "^3.1.0",
    "react-hook-form": "^6.12.2",
    "react-input-mask": "^2.0.4",
    "react-responsive": "^8.1.0",
    "react-router-dom": "^5.2.0",
    "react-toastify": "^6.2.0",
    "react-with-sanctum": "^1.0.0",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0",
    "typescript-plugin-css-modules": "^2.4.0",
    "yup": "^0.31.1"
  }
}
